<Registration_Screen>:
	name:'registration_screen'
	on_enter:
		root.anim4(card)
		root.anim(first_layout)
		root.anim1(second_layout)
		root.anim2(iconbtn)
		root.anim3(label)
		root.keyboard()
		
		
	MDFloatLayout:	
		MDFloatLayout:
			id:first_layout
			size_hint_y:0.6
			pos_hint:{'center_y':1.33}
			radius:[0,0,0,40]
			canvas:
				Color:
					rgba:(180.0/255.0,104.0/255.0,238.0/255.0,1)
				Rectangle:
					size:self.size
					pos:self.pos

		MDFloatLayout:
			id:second_layout
			size_hint:1.5,0.6
			
			pos_hint:{'center_y':1.28,'center_x':0.5}
			radius:[0,0,0,40]
			canvas:
				Color:
					rgba:(180.0/255.0,104.0/255.0,238.0/255.0,1)
				Ellipse:
					size:self.size
					pos:self.pos
		MDIconButton:
			id:iconbtn
			icon:'account-circle'
			pos_hint:{'center_x':0.5,'center_y':0.85}
			user_font_size:'60sp'
			theme_text_color:'Custom'
			text_color:1,1,1,1
		MDLabel:
			id:label
			text:'Registration'
			font_style:'H5'	
			halign:'center'
			markup:True
			theme_text_color:'Custom'
			text_color:1,1,1,1
			pos_hint:{'center_y':0.86}
			opacity:0
		MDCard:			
			id:card
			orientation: "vertical"
			size_hint_x:0.9
			pos_hint:{'center_x':0.5,'center_y':0.28}
			radius:[dp(40),dp(40),40,40]	
			elevation:15
			opacity:0
			MDFloatLayout:				
				MDTextField:
					id:name
	    			hint_text: "Enter Name  "
	    			mode: "rectangle"
	    			size_hint_x:0.8
	    			pos_hint:{'center_y':0.9,'center_x':0.5}
	    			radius:[20,20,20,20]	    	
	    			color_mode: 'custom'
	    			multiline: False	    			
	    			line_anim:True    
       			 line_color_focus: 0,0,0,1 if app.theme_cls.theme_style=='Light' else 1,1,1,1                      			
        	
	    		MDTextField:
	    			id:number
	    			hint_text: "Enter  Number(optional)"
	    			mode: "rectangle"
	    			size_hint_x:0.8
	    			pos_hint:{'center_y':0.8,'center_x':0.5}
	    			radius:[20,20,20,20]	  
	    			multiline: False  	    	
	    			line_anim:True    	
	    			color_mode: 'custom'
	    			input_filter: "int"
        			line_color_focus: 0,0,0,1 if app.theme_cls.theme_style=='Light' else 1,1,1,1                      			
        	
				MDTextField:
					id:email
	    			hint_text: "Enter  Email  "
	    			mode: "rectangle"
	    			size_hint_x:0.8
	    			pos_hint:{'center_y':0.7,'center_x':0.5}
	    			radius:[20,20,20,20]	   
	    			multiline: False	    	
	    			line_anim:True    		
	    			color_mode: 'custom'
        			line_color_focus: 0,0,0,1 if app.theme_cls.theme_style=='Light' else 1,1,1,1                      			
        	
				MDTextField:
					id:password_id
	    			hint_text: "Enter  Password "
	    			mode: "rectangle"
	    			size_hint_x:0.8
	    			pos_hint:{'center_y':0.6,'center_x':0.5}
	    			radius:[20,20,20,20]	   
	    			multiline: False		    			
	    			line_anim:True    
	    			password:True
	    			color_mode: 'custom'
        			line_color_focus: 0,0,0,1 if app.theme_cls.theme_style=='Light' else 1,1,1,1                      			
        		IconToggleButton:
        			id:toggle_btn
					icon:{'normal':'eye-off','down':'eye'}[self.state]						
					on_state:root.show_password()
					pos_hint:{'center_x':0.88,'center_y':0.59}
        			
        			
				MDTextField:
					id:reffer
	    			hint_text: "Referral Code(optional)"
	    			mode: "rectangle"
	    			size_hint_x:0.8
	    			pos_hint:{'center_y':0.5,'center_x':0.5}
	    			radius:[20,20,20,20]	   
	    			multiline: False		    			
	    			current_hint_text_color: 190.0/255.0, 190.0/255.0, 190.0/255.0,1    
	    			color_mode: 'custom'
        			line_color_focus: 0,0,0,1 if app.theme_cls.theme_style=='Light' else 1,1,1,1                      			
        			line_anim:True       
				MagicButton:
					markup:True
					text: f'{" "*int(dp(8))}[b]Sign Up[/b]{" "*int(dp(8))}'
					pos_hint:{'center_x':0.5,'center_y':0.4}     															
					font_size:"10sp"
					on_press:self.shrink()
					md_bg_color:(180.0/255.0,104.0/255.0,238.0/255.0,1)
					on_release:root.loading()
				MDLabel:
					text:f'{" "*int(dp(4))}--- OR ---'
					font_style:'Body2'
					pos_hint:{'center_y':0.32}
					halign:'center'
					theme_text_color:'Custom'
					text_color:(0, 191.0/255.0, 1,1)
				MDLabel:
					text:'Already Have Account?'
					font_style:'Subtitle1'
					pos_hint:{'center_y':0.27}
					halign:'center'
					theme_text_color:'Custom'
					text_color:(0, 0,0,1) if app.theme_cls.theme_style=='Light' else (1,1,1,1)
				MDFlatButton:
				    text: "Login In"
				    theme_text_color: "Custom"
				    text_color: (0, 0, 1, 1) if app.theme_cls.theme_style=='Light' else  (254.0/255.0,97.0/255.0,44.0/255.0,1) 
				    pos_hint:{'center_x':0.8,'center_y':0.27}
				    on_release:root.manager.current='login_screen'
					
					
