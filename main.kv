<MagicButton@MagicBehavior+MDFillRoundFlatButton>:
<MagicCard@MagicBehavior+MDCard>:

<Check@MDCheckbox>:
    group: 'group'
    size_hint: None, None
    size: dp(48), dp(48)
    				
<Content>:
	orientation:"horizontal"
	spacing:50	
	MDSpinner:
		id:spinner
		size_hint:None,None
		size:dp(46),dp(46)
		pos_hint:{'center_x':0.1,'top':1.3}
		active:True
	MDLabel:
		text:'Processing...'
		pos_hint:{'center_x':1.8,'top':1.3}

<Content1>:
	orientation:"horizontal"
	spacing:50	
	MDSpinner:
		id:spinner
		size_hint:None,None
		size:dp(46),dp(46)
